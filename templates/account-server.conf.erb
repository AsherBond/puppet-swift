[DEFAULT]
<% if scope.lookupvar('devices') != :undefined -%>
devices = <%= devices %>
<% end -%>
bind_ip = <%= storage_local_net_ip %>
<% if scope.lookupvar('bind_port') != :undefined -%>
bind_port = <%= bind_port %>
<% end -%>
mount_check = false
user = swift
log_facility = LOG_LOCAL2
workers = 2

[pipeline:main]
pipeline = account-server

[app:account-server]
use = egg:swift#account

[account-replicator]
vm_test_mode = yes

[account-auditor]

[account-reaper]
